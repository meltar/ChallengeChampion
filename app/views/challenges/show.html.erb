<h1>Challenge Complete!</h1>

<div class="challenge">
	<h2><%= @challenge.description %></h2>
</div>

<div class="challenge-result" id="winner">
	<h3>Winner<br>
		<%= @winner.name %>'s <%= champion(@winner) %></h3>
</div>

<div class="challenge-result" id="loser">
	<h3>Loser<br>
	<%= @loser.name %>'s <%= champion(@loser) %></h3>
</div>
